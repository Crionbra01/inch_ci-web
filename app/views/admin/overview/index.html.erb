
<div class="row">

  <div class="col-lg-6">
    <div class="widget">
      <div class="widget-title">
        <i class="fa fa-bars"></i> Statistics
        <a href="#" class="pull-right">Manage</a>
      </div>
      <div class="widget-body medium no-padding" data-scroll-down>
        <div class="table-responsive">
          <table class="table">
            <thead>
              <tr>
                <% @stats_headers.each do |value| %>
                  <th><%= value %></th>
                <% end %>
              </tr>
            </thead>
            <tbody>
              <% @stats.each do |stats| %>
                <tr>
                  <% stats.each do |value| %>
                    <td><%= value %></td>
                  <% end %>
                </tr>
              <% end %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>

  <div class="col-lg-6">
    <div class="widget">
      <div class="widget-title">
        <i class="fa fa-cog"></i> Loading Directive
